lientSecret),\n      },\n      data: \"grant_type=client_credentials\",\n    }).then((res) => {\n      let token = res.data.access_token;\n      dispatch(getToken(token));\n      axios({\n        url: `${url2}?limit=12&market=DE&seed_genres=${genre}&target_instrumentalness=${instrument}&target_valance=${valance}&target_danceability=${dance}&target_mode=${mode}&${tempo}&target_popularity=${popularity}`,\n        method: \"GET\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Accept: \"application/json\",\n          Authorization: \"Bearer \" + token,\n        },\n      }).then((res) => {\n        dispatch(getRecommendations(res.data.tracks));\n        dispatch(currStep(\"Player\"));\n      });\n    });\n  }, []);\n  // [genre, dance, instrument, mode, tempo, popularity, valance, dispatch]);\n\n  return (\n    <div>\n      <StyledLoader>Loading...</StyledLoader>\n    </div>\n  );\n}\n","/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/components/Start.js",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/components/FormContainer.js",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/components/Query.js",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/styled components/styledStart.jsx",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/styled components/styledFormContainer.jsx",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/styled components/styledLoader.jsx",[],"/home/dci/Desktop/practice-dir/portfolio-projects/spotify-playlist/src/styled components/styledQuery.jsx",[],["100","101"],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":48,"column":6,"nodeType":"108","endLine":48,"endColumn":8,"suggestions":"109"},{"ruleId":"102","replacedBy":"110"},{"ruleId":"104","replacedBy":"111"},"no-native-reassign",["112"],"no-n